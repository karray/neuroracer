<?xml version="1.0" encoding="UTF-8"?>
<launch>
    <arg name="always_explore" default="true" />

    <param name="/neuroracer_gym/always_explore" value="$(arg always_explore)" type="boolean"/>
    <!-- This version uses the openai_ros environments -->
    <rosparam command="load" file="$(find qlearning)/config/params.yaml" />
    <!-- Launch the training system -->
    <node pkg="qlearning" name="neuroracer" type="start_qlearning.py" output="screen"/>
</launch>
